- if @information.errors.any?
  = render partial: 'alert_error', locals: { model: @information }
= form_for :information, method: 'post', url: admin_inforamtions_path, html: { class: 'form-horizontal', role: 'form' } do |t|
  %h2 Create new information
  = render partial: 'form', locals: { t: t}
  .form-group
    .col-sm-offset-3
      = t.submit 'Submit!', class: ['btn', 'btn-md', 'btn-primary']

